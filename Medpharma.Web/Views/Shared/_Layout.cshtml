    <script src="~/assets/js/config.js"></script>

    
    <!-- Colocar no JS -->
    <script type="text/javascript">
        var loadFile = function (event) {
            var image = document.getElementById('uploadedAvatar');
            image.src = URL.createObjectURL(event.target.files[0]);
